    "/config": {
      "get": {
        "description": "Gets the currently defined configuration",
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http"
        ],
        "tags": [
          "System_Management_Agent"
        ],
        "summary": "Config",
        "operationId": "Config",
        "responses": {
          "200": {
            "description": "Get configuration",
            "schema": {
              "$ref": "#/definitions/ConfigurationStruct"
            }
          }
        }
      }
    },
    "/metrics": {
      "get": {
        "description": "Gets the current metrics",
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http"
        ],
        "tags": [
          "System_Management_Agent"
        ],
        "summary": "Metrics",
        "operationId": "Metrics",
        "responses": {
          "200": {
